{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Jobs",
    "description": "Job advertisement. It describes the requirements for an advertised position and is aimed at applicants."
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": false,
    "autoPK": false,
    "autoCreatedAt": true,
    "autoUpdatedAt": true
  },
  "pluginOptions": {},
  "attributes": {
    "organization": {
      "type": "string",
      "maxLength": 100
    },
    "dateDeleted": {
      "type": "datetime"
    },
    "jobId": {
      "type": "uid",
      "primaryKey": true,
      "required": true
    },
    "location": {
      "type": "json"
    },
    "intro": {
      "type": "richtext"
    },
    "tasks": {
      "type": "richtext"
    },
    "offer": {
      "type": "richtext"
    },
    "jobTitle": {
      "type": "string"
    },
    "reference": {
      "type": "string"
    },
    "applyEmail": {
      "type": "email"
    },
    "applyUrl": {
      "type": "string"
    },
    "formattedAddress": {
      "type": "string"
    },
    "applyPost": {
      "type": "boolean"
    },
    "introLabel": {
      "type": "string"
    },
    "taskLabel": {
      "type": "string"
    },
    "profileLabel": {
      "type": "string"
    },
    "offerLabel": {
      "type": "string"
    },
    "contactInfoLabel": {
      "type": "string"
    },
    "contactInfo": {
      "type": "richtext"
    },
    "html": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "jobs"
    },
    "meta": {
      "type": "json"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "header": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "org": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization.organization"
    },
    "profile": {
      "type": "richtext"
    }
  }
}
