{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Jobs",
    "description": "Job advertisement. It describes the requirements for an advertised position and is aimed at applicants."
  },
  "options": {
    "draftAndPublish": true,
    "populateCreatorFields": true,
    "autoPK": false,
    "autoCreatedAt": true,
    "autoUpdatedAt": true
  },
  "pluginOptions": {},
  "attributes": {
    "organization": {
      "type": "string",
      "maxLength": 100
    },
    "dateDeleted": {
      "type": "datetime"
    },
    "jobId": {
      "type": "uid",
      "primaryKey": true,
      "required": true
    },
    "jobUser": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job-user.job-user",
      "inversedBy": "jobs"
    },
    "location": {
      "type": "json"
    },
    "intro": {
      "type": "richtext"
    },
    "tasks": {
      "type": "richtext"
    },
    "offer": {
      "type": "richtext"
    },
    "jobTitle": {
      "type": "string"
    },
    "reference": {
      "type": "string"
    },
    "salary": {
      "type": "json"
    },
    "applyEmail": {
      "type": "email"
    },
    "applyUrl": {
      "type": "string"
    },
    "formattedAddress": {
      "type": "string"
    },
    "workKind": {
      "type": "json"
    },
    "applyPost": {
      "type": "boolean"
    },
    "partTimePercentage": {
      "type": "integer"
    },
    "shiftWorkAmount": {
      "type": "integer"
    },
    "salaryVisibility": {
      "type": "boolean"
    },
    "introLabel": {
      "type": "string"
    },
    "taskLabel": {
      "type": "string"
    },
    "profileLabel": {
      "type": "string"
    },
    "offerLabel": {
      "type": "string"
    },
    "contactInfoLabel": {
      "type": "string"
    },
    "contactInfo": {
      "type": "richtext"
    },
    "workDuration": {
      "type": "json"
    },
    "html": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
