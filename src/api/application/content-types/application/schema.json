{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "Applications",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstname": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "job": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::job.job",
      "inversedBy": "applications"
    },
    "phone": {
      "type": "string"
    },
    "attachments": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "organization": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::organization.organization",
      "inversedBy": "applications"
    },
    "users_permissions_users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "applications"
    },
    "extras": {
      "type": "json"
    },
    "summary": {
      "type": "richtext"
    },
    "street": {
      "type": "string"
    },
    "housenumber": {
      "type": "string"
    },
    "postalcode": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "diverse"
      ]
    }
  }
}
